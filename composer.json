{
	"name": "coffeephp/binary",
	"description": "Provides classes for handling PHP-Serialized (Binary) data.",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Danny Damsky",
			"email": "dannydamsky99@gmail.com"
		}
	],
	"keywords": [
		"coffeephp",
		"coffee",
		"php",
		"serialized",
		"php-serialized",
		"binary",
		"translator",
		"serialize",
		"unserialize",
		"deserialize",
		"serialization",
		"unserialization",
		"deserialization"
	],
	"require": {
		"php": ">=7.4",
		"ext-apcu": "*",
		"coffeephp/edi": "1.*"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"coffeephp/component-registry": "1.*",
		"phpunit/phpunit": "9.*"
	},
	"autoload": {
		"psr-4": {
			"CoffeePhp\\Binary\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CoffeePhp\\Binary\\Test\\Integration\\": "test/Integration/",
			"CoffeePhp\\Binary\\Test\\Unit\\": "test/Unit/"
		}
	},
	"suggest": {
		"coffeephp/component-registry": "For integration of the component with the CoffeePHP framework."
	},
	"config": {
		"sort-packages": true,
		"optimize-autoloader": true,
		"apcu-autoloader": true
	},
	"extra": {
		"coffeephp": {
			"registry": [
				{
					"registrar": "CoffeePhp\\Binary\\Integration\\BinaryComponentRegistrar"
				}
			]
		}
	}
}
